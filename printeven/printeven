function findEvenNumber(arr) {
  let total = 0;
  let x = [];
  for (let i = 0; i < arr.length; i++) {
    if (arr[i] % 2 === 0) {
      total += arr[i];
      x.push(arr[i])
    }
  }

  if(x.length>0){
    return total;
  } else{
    return false
  }
 
}

const numbers = [1, 3, 5, 7,12,15,17,19,22,25];
console.log(findEvenNumber(numbers)); 




